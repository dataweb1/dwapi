{% if settings.url != "" %}
    {% if status.success == true %}
    {{ settings.url }}?from=dwapi_{{ parameters.path.endpoint }}_{{ parameters.path.action }}&status=true&error_code=&hash={{ parameters.query.hash }}&temp_token={{ parameters.query.temp_token }}
    {% else %}
    {{ settings.url }}?from=dwapi_{{ parameters.path.endpoint }}_{{ parameters.path.action }}&status=false&error_code={{ status.error_code }}&hash={{ parameters.query.hash }}&temp_token={{ parameters.query.temp_token }}
    {% endif %}
{% endif %}